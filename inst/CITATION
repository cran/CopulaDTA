citHeader("To cite CopulaDTA in publications use:")

citEntry(entry = "Manual",
  title        = "CopulaDTA: Copula based bivariate beta-binomial model for diagnostic test accuracy studies",
  author       = personList(as.person("Victoria N Nyaga")),
  year         = "2015",
  note 		   = "R package version 0.0.2",
  url          = "https://cran.r-project.org/package=CopulaDTA",
  textVersion  =
		paste("Victoria N Nyaga (2015).",
        "CopulaDTA: Copula based bivariate beta-binomial model for diagnostic test accuracy studies.",
        "R package version 0.0.2.", "URL: https://cran.r-project.org/package=CopulaDTA"))


